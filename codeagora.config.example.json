{
  "reviewers": [
    {
      "id": "reviewer-1",
      "name": "Codex Reviewer",
      "backend": "codex",
      "model": "o4-mini",
      "enabled": true,
      "timeout": 120
    },
    {
      "id": "reviewer-2",
      "name": "Gemini Reviewer",
      "backend": "gemini",
      "model": "gemini-2.5-flash",
      "enabled": true,
      "timeout": 120
    },
    {
      "id": "reviewer-3",
      "name": "Kimi Reviewer (via OpenCode)",
      "backend": "opencode",
      "model": "opencode/kimi-k2.5-free",
      "enabled": true,
      "timeout": 120
    },
    {
      "id": "reviewer-4",
      "name": "Grok Reviewer (via OpenCode)",
      "backend": "opencode",
      "model": "github-copilot/grok-3-mini",
      "enabled": true,
      "timeout": 120
    }
  ],
  "settings": {
    "min_reviewers": 4,
    "max_parallel": 6,
    "output_format": "terminal",
    "log_dir": ".codeagora/logs",
    "debate": {
      "enabled": true,
      "majority_threshold": 0.75,
      "max_rounds": 3,
      "early_stop": true
    }
  }
}
