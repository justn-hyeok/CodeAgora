{
  "head_agent": {
    "provider": "github-copilot",
    "model": "claude-sonnet-4.5",
    "fallback_model": "claude-haiku-4.5"
  },
  "supporters": {
    "codex": {
      "provider": "github-copilot",
      "model": "gpt-5.3-codex",
      "enabled": true,
      "categories": ["security", "performance", "architecture"]
    },
    "gemini": {
      "provider": "google",
      "model": "antigravity-gemini-3-flash",
      "enabled": true,
      "categories": ["best-practices", "code-quality", "testing"]
    }
  },
  "reviewers": [
    {
      "name": "haiku-1",
      "provider": "github-copilot",
      "model": "claude-haiku-4.5",
      "enabled": true,
      "timeout": 120
    },
    {
      "name": "haiku-2",
      "provider": "github-copilot",
      "model": "claude-haiku-4.5",
      "enabled": true,
      "timeout": 120
    },
    {
      "name": "kimi-1",
      "provider": "opencode",
      "model": "kimi-k2.5-free",
      "enabled": true,
      "timeout": 120
    },
    {
      "name": "kimi-2",
      "provider": "opencode",
      "model": "kimi-k2.5-free",
      "enabled": true,
      "timeout": 120
    },
    {
      "name": "minimax-1",
      "provider": "opencode",
      "model": "minimax-m2.5-free",
      "enabled": true,
      "timeout": 120
    },
    {
      "name": "minimax-2",
      "provider": "opencode",
      "model": "minimax-m2.5-free",
      "enabled": true,
      "timeout": 120
    },
    {
      "name": "gemini-1",
      "provider": "google",
      "model": "gemini-3-flash-preview",
      "enabled": true,
      "timeout": 120
    },
    {
      "name": "gemini-2",
      "provider": "google",
      "model": "gemini-3-flash-preview",
      "enabled": true,
      "timeout": 120
    }
  ],
  "discord": {
    "enabled": false,
    "webhook_url": ""
  },
  "github": {
    "enabled": false,
    "token": "",
    "owner": "",
    "repo": ""
  },
  "settings": {
    "min_reviewers": 4,
    "max_parallel": 5,
    "output_format": "markdown",
    "default_timeout": 300,
    "backend": "opencode",
    "debate": {
      "enabled": true,
      "min_confidence_diff": 0.3,
      "max_rounds": 3
    },
    "synthesis": {
      "dedup_threshold": 0.7,
      "min_confidence": 0.3
    }
  }
}
